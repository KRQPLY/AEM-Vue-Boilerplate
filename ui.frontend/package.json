    "floating-vue": "^2.0.0-beta.8",
    "focus-trap": "^6.7.3",
    "lodash": "^4.17.21",
    "open-props": "^1.3.8",
    "pdfmake": "^0.2.4",
    "pinia": "^2.0.30",
    "qrcode": "^1.5.0",
    "vue": "^3.2.29",
