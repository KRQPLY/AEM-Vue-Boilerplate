    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.57.1",
